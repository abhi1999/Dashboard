{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules":{
    "variable-name": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-debugger": false,
    "jsx-no-lambda": false,
    "no-bitwise" : false,
    "no-string-literal" : false,
    "ordered-imports": [
      false,
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ]
  },
  "jsRules":{
    "no-console": false,
    "jsx-no-lambda":false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
