{
  "name": "vp5-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start1": "set PORT=9005 && react-app-rewired start",
    "start": "set PORT=9005 && react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-scripts-ts eject",
    "test-build": "http-server ./build",
    "serve": "serve -s build",
    "postinstall": "npm run build"
  },
  "dependencies": {
